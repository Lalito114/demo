<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Puntada.Acumular.ClaveDespachadorAcumular">

    <TextView
        android:id="@+id/textView22"
        android:layout_width="301dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="22dp"
        android:layout_marginBottom="364dp"
        android:text="INGRESA CLAVE DE DESPACHADOR"
        android:textColor="@color/button_unselected"
        android:textSize="18sp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="168dp"
        android:layout_height="92dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="21dp"
        android:layout_marginBottom="461dp"
        app:srcCompat="@drawable/logopuntada" />

    <EditText
        android:id="@+id/edtDespachadorclave"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="70dp"
        android:layout_marginBottom="277dp"
        android:background="@drawable/cajadetextopuntada"
        android:ems="10"
        android:gravity="center_horizontal"
        android:hint="******"
        android:inputType="numberPassword" />

    <Button
        android:id="@+id/btnSiguientePuntada"
        android:layout_width="144dp"
        android:layout_height="54dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="104dp"
        android:layout_marginBottom="183dp"
        android:background="@drawable/fondoazulpuntada"
        android:text="Siguiente"
        android:textColor="@color/a_normal" />

</RelativeLayout>